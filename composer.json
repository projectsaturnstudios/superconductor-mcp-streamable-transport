{
    "name": "superconductor/streamable-http-transport",
    "description": "Streamable HTTP transport protocol for the Superconductor MCP framework.",
    "type": "library",
    "version": "20241105.5.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Project Saturn Studios, LLC",
            "email": "info@projectsaturnstudios.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "superconductor/mcp": "^20241105.5.0",
        "lorisleiva/laravel-actions": "^2.6",
        "projectsaturnstudios/quickuuid": "^0.2.0",
        "projectsaturnstudios/laravel-design-patterns": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Superconductor\\Transports\\StreamableHttp\\": "src/"
        },
        "files": [
            "src/Helpers/streamable_routing.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Superconductor\\Transports\\StreamableHttp\\Providers\\StreamableHttpTransportServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "react/event-loop": "^1.5",
        "react/stream": "^1.4",
        "illuminate/container": "^12.0",
        "illuminate/support": "^12.24",
        "illuminate/events": "^12.24"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repositories": {
        "monorepo": {
            "type": "path",
            "url": "../../*/*"
        }
    }
}
